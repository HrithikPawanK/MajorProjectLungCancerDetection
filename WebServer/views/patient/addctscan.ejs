<% layout('layouts/boilerplate') %> 
<h3>Upload CT Scan</h1>
<form action="/patient/addctscan" method="post" enctype="multipart/form-data">
    <div class="mt-3 mb-3">
        <input class="form-control" type="file" id="ctscan" name="ctscan" required>
    </div>
    <button class="btn btn-success">Upload</button>
</form>
